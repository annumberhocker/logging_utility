{
  "openapi": "3.0.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0",
    "description": "Logging API"
  },
  "servers": [
    {
      "url": "https://{route_url}",
      "description": "The route to the application hosting the logging_utility api",
      "variables": {
        "route_url": {
          "default": "logging-service.1m7e1ln410px.ca-tor.codeengine.appdomain.cloud",
          "description": "The portion of the Route URL that follows https://"
        }
      }
    }
  ],
  "paths": {
    "/log-to-csv": {
      "post": {
        "description": "send JSON results for logging",
        "summary": "logging_utility",
        "operationId": "logging_utility_logging_utility_post",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/logging_utility_response"
                }
              }
            },
            "description": "Successful Response"
          }
        },
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/logging_utility_request"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "logging_utility_request": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "title": "query"
          },
          "llm_response": {
            "type": "string",
            "title": "llm_response"
          },
          "rating": {
            "type": "string",
            "title": "rating"
          },
          "comments": {
            "type": "string",
            "title": "comments"
          }
        },
        "required": [
          "query",
          "llm_response",
          "rating",
          "comments"
        ],
        "title": "logging_utility_request"
      },
      "logging_utility_response": {
        "type": "object",
        "properties": {
          "response": {
            "type": "object",
            "title": "Response"
          }
        },
        "title": "logging_utility_response"
      }
    }
  }
}
